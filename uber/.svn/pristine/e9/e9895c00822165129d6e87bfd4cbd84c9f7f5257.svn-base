/**
 * 项目管理-项目计划-节点计划-导入节点
 */
Ext.define('ExtApp.view.projectManagement.depttasklist.ImportTaskList',{
	extend:'Ext.window.Window',
	xtype:'importTaskList',

	modal: true,
	closable: false,
	
	layout:{
		type:'fit'
	},
	
	controller: 'importTaskList',
	
	width: 400,
	height: 150,
	
	title: '导入节点计划',
	
	initComponent: function() {
		
		this.items = [{
			xtype:'form',
		    margin: 20,
		    layout: 'column',
		    items: [{
		        xtype: 'filefield',
		        name: 'upload',
		        msgTarget: 'side',
		        allowBlank: false,
		        columnWidth:1,
		        buttonText: '浏览'
		    }]
		}];
		this.tools = [{
			text:'下载模板',
			xtype: 'button',
			iconCls: 'x-fa fa-download',
			handler: 'downloadTemplate'
		},{
			text:'导入',
			xtype: 'button',
			iconCls: 'x-fa fa-circle-o',
			handler: 'confirm'
		},{
			text:'关闭',
			xtype: 'button',
			iconCls: 'x-fa fa-close',
			handler: 'closeWindow'
		}];
		this.callParent();
	}
});