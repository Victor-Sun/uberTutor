Ext.define('ExtApp.view.projectmanagement.extplan.ext301.PartExcuteControlList',{
	extend: 'Ext.grid.Panel',
	xtype: 'partExcuteControlList',
	requires: [
       'Ext.selection.CellModel'
   ],
	controller: 'partExcutePanelControll',
	selModel: 'checkboxmodel',
	listeners:{
		edit: 'edit'
	},
	initComponent: function() {
		var me = this;
		var partExcutePoolStore = Ext.create('ExtApp.store.projectmanagement.extplan.ext301.PartExcutePoolStore');
		partExcutePoolStore.load({params:{code:me.code,programVehicleId:me.programVehicleId}});
		me.store = partExcutePoolStore;
		me.columns = [{
			text: '零件号',
			dataIndex: 'ITEM_NO',
			width:100
		},{
			text: '零件名称',
			dataIndex: 'ITEM_NAME',
			flex: 1
		},{
			text: 'KCRP',
			dataIndex: 'IS_KCRP',
			flex: 1
		},{
			text: '负责主体',
			dataIndex: 'OBS_NAME',
			flex: 1
		},{
			text: '责任工程师',
			dataIndex: 'ITEM_OWNER',
			flex: 1
		},{
			text: me.code,
			columns:[{
				text: '计划完成',
				format: 'Y/m/d',
				dataIndex: 'planFinishDate',
				width:200
			},{
				xtype: 'datecolumn',
				text: '实际完成',
				dataIndex: 'actualFinishDate',
				width:200,
				format: 'Y/m/d',
				editor: {
		            xtype: 'datefield',
		            format: 'Y/m/d'
		        }
			}]
		}];
		me.plugins=[Ext.create('Ext.grid.plugin.CellEditing', {clicksToEdit: 1})];
		me.callParent(arguments);
	}
});