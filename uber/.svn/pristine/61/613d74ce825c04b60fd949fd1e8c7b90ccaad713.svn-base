Ext.define('ExtApp.view.projectmanagement.cp.ProjectValveDocument',{
	extend:'Ext.panel.Panel',
	xtype:'projectValveDocument',

	controller: 'projectValveDocument',
	layout: 'fit',
	
	initComponent: function() {
		var me = this;
		me.store = Ext.create(
			'ExtApp.store.projectmanagement.ProjectValveDocumentStore');
		me.store.getProxy().extraParams = {
			programId: this.programId,
			programVehicleId: this.programVehicleId
		};
		me.store.load();
		
		me.items = [{
			xtype: 'panel',
			scrollable:true,
			reference: 'refProjectValveDocument',
			layout:{
				type:'hbox',
				align:'stretch'
			}
//			tbar: [{
//                text: '全部展开',
//                iconCls: '',
//                handler: 'onExpandAllClick'
//            },{
//                text: '全部关闭',
//                iconCls: '',
//                handler: 'onCollapseAllClick'
//            }]
        }];
		this.callParent(arguments);
	}
});