/**
 * 专项管理-具体专项页面
 */
Ext.define('ExtApp.view.projectmanagement.extplan.ext201.Ext201StagePanel',{
	extend:'Ext.panel.Panel',
	xtype:'ext201StagePanel',
	margin:10,
	cls:'shadow',
//	title:'ddd',
	frame:false,
	border:0,
	initComponent: function() {
		var me = this;
		me.title = me.text;
		var partList = Ext.create('ExtApp.view.projectmanagement.extplan.ext201.StagePartList',{stageId:me.nodeId});
		var partProgressList = Ext.create('ExtApp.view.projectmanagement.extplan.ext201.StagePartProgressList',{stageId:me.nodeId,partList:partList});
		me.items = [partProgressList,partList];
		me.callParent(arguments); 
	}
});