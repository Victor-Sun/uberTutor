/**
 * 项目管理-阀门交付物
 */
Ext.define('ExtApp.view.projectManagement.cp.PmDeliverableBase',{
	extend:'Ext.tab.Panel',
	xtype:'pmDeliverableBase',

	initComponent: function() {
		var me = this;
		
		this.items = [{
			xtype: 'projectValveDocument',
			title: '图形模式',
			programId: me.programId,
        	programVehicleId: me.programVehicleId,
        	nodeId: me.nodeId,
        	text: me.text,
        	record: me.record,
        	projectPrivilege: me.projectPrivilege
		},{
			xtype: 'pmDeliverableList',
			title: '列表模式',
			programVehicleId: me.programVehicleId
		}];
		
		this.callParent();
	}

});