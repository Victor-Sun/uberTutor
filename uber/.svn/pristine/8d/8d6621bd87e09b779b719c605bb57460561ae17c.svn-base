        if (renderData.endsOutsideView)  {
            internalCls += ' sch-event-endsoutside ';
        }
        
        if (renderData.startsOutsideView)  {
            internalCls += ' sch-event-startsoutside ';
        }
        
        if (this.eventBarIconClsField)  {
            internalCls += ' sch-event-withicon ';
        }
        
