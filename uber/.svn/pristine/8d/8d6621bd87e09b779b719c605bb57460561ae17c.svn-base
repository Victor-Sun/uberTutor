                break;
            case 'vertical':
                this.addCls([
                    'sch-vertical-resource',
                    'sch-vertical'
                ]);
                this.removeCls([
                    'sch-calendar',
                    'sch-horizontal'
                ]);
                break;
            case 'calendar':
                this.addCls([
                    'sch-calendar',
                    'sch-vertical'
                ]);
