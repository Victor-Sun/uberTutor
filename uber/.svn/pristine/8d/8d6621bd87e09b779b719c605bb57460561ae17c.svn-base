            resizer.resizeTracker.resize = function(box) {
                var scrollDelta;
                if (isVertical) {
                    scrollDelta = s.getScrollY() - me.startScroll.top;
                    if (handlePos[0] === 's') {
                        box.y -= scrollDelta;
                    }
                    box.height += Math.abs(scrollDelta);
                } else {
                    scrollDelta = s.getScrollX() - me.startScroll.left;
