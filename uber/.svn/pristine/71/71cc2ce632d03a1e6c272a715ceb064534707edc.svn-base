package com.gnomon.pdms.entity;

import java.math.BigDecimal;
import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.SequenceGenerator;
import javax.persistence.Table;

@Entity
@Table(name="PM_EXT301_ITEM")
public class Ext301ItemEntity {
	
	@Id
	@GeneratedValue(strategy = GenerationType.AUTO,generator="PM_EXT301_ITEM_SEQ")     
	@SequenceGenerator(name="PM_EXT301_ITEM_SEQ", sequenceName="PM_EXT301_ITEM_SEQ")
	private Long id;
	
	//
	@Column(name="EXT_PROJECT_ID") 
	private Long extProjectId;	
	
	@Column(name="ITEM_NO") 
	private String itemNo;	
	
	@Column(name="ITEM_NAME") 
	private String itemName;	
	
	@Column(name="OBS_ID") 
	private String obsId;	
	
	@Column(name="OWNER") 
	private String owner;	
	
	@Column(name="SOURCING_TYPE") 
	private String sourcingType;	
	
	@Column(name="SEVERITY_LEVEL") 
	private String severityLevel;	
	
	@Column(name="DEV_TYPE") 
	private String devType;	
	
	@Column(name="DV_STATUS") 
	private String dvStatus;	
	
	@Column(name="IS_SE") 
	private String isSe;	
	
	@Column(name="PRODUCTION_TOOLING_PERIOD") 
	private Long productionToolingPeriod;	
	
	@Column(name="IS_ACTIVE") 
	private String isActive;
	
	@Column(name="PROGRESS_STATUS") 
	private String progressStatus;

	@Column(name="IS_KCRP") 
	private String isKcrp;
	
	@Column(name="LIFECYCLE_STATUS") 
	private String lifecycleStatus;
	
	@Column(name="STATUS_CODE") 
	private String statusCode;
	
	@Column(name="CREATE_DATE") 
	private Date createDate;
	
	@Column(name="UPDATE_DATE") 
	private Date updateDate;
	
	@Column(name="UPDATE_BY") 
	private String updateBy;
	
	@Column(name="PROGRESS") 
	private BigDecimal progress;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Long getExtProjectId() {
		return extProjectId;
	}

	public void setExtProjectId(Long extProjectId) {
		this.extProjectId = extProjectId;
	}

	public String getItemNo() {
		return itemNo;
	}

	public void setItemNo(String itemNo) {
		this.itemNo = itemNo;
	}

	public String getItemName() {
		return itemName;
	}

	public void setItemName(String itemName) {
		this.itemName = itemName;
	}

	public String getObsId() {
		return obsId;
	}

	public void setObsId(String obsId) {
		this.obsId = obsId;
	}

	public String getOwner() {
		return owner;
	}

	public void setOwner(String owner) {
		this.owner = owner;
	}

	public String getSourcingType() {
		return sourcingType;
	}

	public void setSourcingType(String sourcingType) {
		this.sourcingType = sourcingType;
	}

	public String getSeverityLevel() {
		return severityLevel;
	}

	public void setSeverityLevel(String severityLevel) {
		this.severityLevel = severityLevel;
	}

	public String getDevType() {
		return devType;
	}

	public void setDevType(String devType) {
		this.devType = devType;
	}

	public String getDvStatus() {
		return dvStatus;
	}

	public void setDvStatus(String dvStatus) {
		this.dvStatus = dvStatus;
	}

	public String getIsSe() {
		return isSe;
	}

	public void setIsSe(String isSe) {
		this.isSe = isSe;
	}

	public Long getProductionToolingPeriod() {
		return productionToolingPeriod;
	}

	public void setProductionToolingPeriod(Long productionToolingPeriod) {
		this.productionToolingPeriod = productionToolingPeriod;
	}

	public String getIsActive() {
		return isActive;
	}

	public void setIsActive(String isActive) {
		this.isActive = isActive;
	}

	public String getProgressStatus() {
		return progressStatus;
	}

	public void setProgressStatus(String progressStatus) {
		this.progressStatus = progressStatus;
	}

	public String getIsKcrp() {
		return isKcrp;
	}

	public void setIsKcrp(String isKcrp) {
		this.isKcrp = isKcrp;
	}

	public String getLifecycleStatus() {
		return lifecycleStatus;
	}

	public void setLifecycleStatus(String lifecycleStatus) {
		this.lifecycleStatus = lifecycleStatus;
	}

	public String getStatusCode() {
		return statusCode;
	}

	public void setStatusCode(String statusCode) {
		this.statusCode = statusCode;
	}

	public Date getCreateDate() {
		return createDate;
	}

	public void setCreateDate(Date createDate) {
		this.createDate = createDate;
	}

	public Date getUpdateDate() {
		return updateDate;
	}

	public void setUpdateDate(Date updateDate) {
		this.updateDate = updateDate;
	}

	public String getUpdateBy() {
		return updateBy;
	}

	public void setUpdateBy(String updateBy) {
		this.updateBy = updateBy;
	}

	public BigDecimal getProgress() {
		return progress;
	}

	public void setProgress(BigDecimal progress) {
		this.progress = progress;
	}
	
	
	
}
