/**
 * 待办任务-附件
 */
Ext.define('ExtApp.view.myworkspace.task.PmTaskAttachment',{
	extend:'Ext.form.Panel',
	xtype:'pmTaskAttachment',
	
	controller: 'pmTaskAttachment',
	
	title: '附件',
	
	initComponent: function() {
		this.items = [{
			xtype: 'fileAttachment',
			programId: this.programId,
    		sourceType: 'SOURCE_TYPE_TASK_ATTACHMENT',
    		sourceId: this.taskId,
    		parentCallerView: 'pmTaskAttachment'
		}];
		this.callParent(arguments);
	}
});