/**
 * 项目管理-项目计划-节点计划
 */
Ext.define('ExtApp.view.projectManagement.depttasklist.DeptPlanTaskBase',{
	extend:'Ext.panel.Panel',
	xtype:'deptPlanTaskBase',
	
	controller: 'deptPlanTaskBase',
	
	layout: 'fit',
    
	initComponent: function() {
		var me = this;
		
		me.title = me.record.get('vehicleName') + ' <i class="x-fa fa-arrow-right" style="font-style: none;"></i>' +' 项目主计划 <i class="x-fa fa-arrow-right" style="font-style: none;"></i> 节点计划';
		if (me.record.get('planLevel') == '2') {
			me.title = me.record.get('vehicleName') + ' <i class="x-fa fa-arrow-right" style="font-style: none;"></i>' + ' 项目二级计划' + ' <i class="x-fa fa-arrow-right" style="font-style: none;"></i> ' + me.record.get('fnGroupName') + ' <i class="x-fa fa-arrow-right" style="font-style: none;"></i>' + ' 节点计划';
		}
		
		this.callParent();
	}
});