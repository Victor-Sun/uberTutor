.search-item {
	width: 100%;
    padding: 2px;
    white-space: normal;
    color: #555;
    text-decoration: none;
    display: inline-block;
    overflow: hidden;
    
    h3 {
	    display: block;
	    font: inherit;
	    font-weight: bold;
	    color: #222;
	    margin-bottom: 0px !important;
	    margin-top: 2px !important;
    }
    
    span {
    	display: block;
	    font-weight: normal;
	    margin:0 0 5px 5px;
	    min-width: 150px;
	    line-height: 16px;
    }
    
    .results-left {
		float: left;
		width: 75%;
		word-wrap: break-word;
	}
    
    .results-right {
    	float: right;
    	clear: none;
    	width: 100px;
    	margin-top: 2px !important;
    }
}

