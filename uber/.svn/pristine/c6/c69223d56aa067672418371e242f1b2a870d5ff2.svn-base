.ims-grid {
	.x-grid-item {
		border-color: transparent;
	}

	.x-grid-cell {
		border-color: transparent;
	}
}

.one-page-report body:after {
	margin: 0 !important;
}

.profile-image {
	border-radius: 20px;
}

.workorder {
	background-color: #fff;
	border-color: #9d9d9d;
	border-style: solid;
	border-width: 0px 1px 2px 1px !important;
}

.toolbar-grey {
	background-color: #e6e6e6;
}

@if $theme-name == 'theme-classic' or $theme-name == 'theme-gray' {
    .cell-editing-delete-row {
        background-image: url(images/icons/fam/delete.gif);
    }
} @else {
    .cell-editing-delete-row:before {
        font: 16px/1 FontAwesome;
        color: $color-soft-red;
        content: $fa-var-minus-circle;
    }
    
    .icon-save:before {
        font: 16px/1 FontAwesome;
        content: $fa-var-save;
        color: $color-soft-blue;
    }
    
    .icon-add:before {
        font: 16px/1 FontAwesome;
        color: $color-soft-green;
        content: $fa-var-plus-circle;
    }
    
    .icon-file:before {
        font-family: FontAwesome;
        content: $fa-var-file-o;
        color: $color-soft-green;
    }
    
    .icon-files:before {
        font-family: FontAwesome;
        content: $fa-var-files-o;
        color: $color-soft-green;
    }
    
    .icon-share-a:before {
        font-family: FontAwesome;
        content: $fa-var-share-square-o;
        color: $color-soft-cyan;
    }
    
    .icon-share-b:before {
        font-family: FontAwesome;
        content: $fa-var-share-square-o;
        color: $color-soft-purple;
    }

    .icon-search:before {
        font: 16px/1 FontAwesome;
        content: $fa-var-search;
        color: $color-soft-green;
    }
    
    .text-readonly {
        background-color: #f0f0f0;
        color: #949495;
    }
    
    .icon-cancel:before {
        font: 16px/1 FontAwesome;
        content: $fa-var-undo;
        color: $color-soft-green;
    }
}
