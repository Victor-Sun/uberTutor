Ext.define('ExtApp.view.projectmanagment.vehicle.ChartHelp',{
	extend: 'Ext.window.Window',
	xtype: 'chartHelp',
	
	controller: 'chartHelp',
	
	height: 300,
	width: 300,
	header: false,
	
	initComponent: function() {
		var help = '';
		if (this.chart == 'taskProgressChart') {
			help = '节点进度趋势图说明';
		} else if (this.chart == 'openIssueTrendChart') {
			help = '项目趋势图说明';
		} else if (this.chart == 'nodeTrendChart') {
			help = '节点分布图说明';
		}
		
		this.html = help;
		
		this.callParent();
	}
})