/**
 * 首页-多项目状态报告
 */
Ext.define('ExtApp.view.dashboard.ProjectReportController',{
    extend: 'ExtApp.view.common.BaseViewController',
    alias: 'controller.projectReport',
    
    //双击列表
    onCelldblclick: function(grid , td , cellIndex , record , tr , rowIndex , e , eOpts) {
    	var rec = grid.getStore().getAt(rowIndex);
    	var win = Ext.create({
    		xtype: 'projectWindow',
    		programId: rec.get('programId'),
			programCode: rec.get('programCode'),
			programName:rec.get('programName')
    	});
    	win.show();
    }
    
});
