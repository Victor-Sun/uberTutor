Ext.define('ExtApp.view.projectmanagement.extplan.ext202.Ext202PanelController', {
    extend: 'Ext.app.ViewController',
    alias: 'controller.ext202Panel',
    itemdblclick:function(grid , record , item , index , e , eOpts){
    	var me = this;
    	var projectId = me.getView().nodeId;
    	var win = Ext.create('ExtApp.view.projectManagement.extplan.ext202.Ext202Form', {projectId:projectId,record:record});
    	win.show();
    }
});
