Ext.define('Ext.chart.theme.MyFancy',{  
	extend: 'Ext.chart.theme.Base',    
	alias:'chart.theme.MyFancy',
	singleton: true,
    constructor: function(config) {  
        this.callParent([Ext.apply({  
            colors: ['#FFC000','#F4F207','#B2EC0A','#4DD60C','#00B050','#1C981C']  
        }, config)]);  
    }  
});