Ext.define('ExtApp.view.qualityissuemanagement.imscode.ImsCodeWindow',{
	xtype:'imsCodeWindow',
	extend:'Ext.window.Window',
	controller:'testType',
	width:500,
	height:400,
	// margin:'65 0 0 250',
	title:'试验类型',
	fbar: [
	       { type: 'button', text: '保存', handler: 'onButtonClickSubmit' },
	       { type: 'button', text: '关闭', handler: 'closeWindow' }
	     ],
	initComponent: function() {
		me = this;
    	me.items = [{
			xtype:'form',
			reference: 'testTypeForm',
			layout:{
				type:'vbox',
				align:'stretch'
			},
			items:[{
		        xtype:'hiddenfield',
		        name: 'id'
			},{
				xtype:'textfield',
				name:'code',
				padding:'10 25 10 30',
				labelAlign:'top', 
				allowBlank:false, 
				fieldLabel:'试验类型编号'
			},{
				xtype:'textfield',
				name:'name',
				padding:'10 25 10 30',
				labelAlign:'top', 
				allowBlank:false, 
				fieldLabel:'试验类型名称'
			}]
		}];
    	me.callParent(arguments);
	}
});