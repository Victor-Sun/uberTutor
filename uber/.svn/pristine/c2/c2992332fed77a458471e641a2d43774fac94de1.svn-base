/**
 * 项目管理-项目模板库-主计划(添加模板)Controller
 */
Ext.define('ExtApp.view.projectmanagement.templatelibrary.ProjectTemplateAddController',{
    extend: 'Ext.app.ViewController',
    alias: 'controller.projectTemplateAdd',
    
    //添加
    save: function(btn) {
    	var form1 = this.lookupReference('refprojectTemplateAdd');
    	var formName = form1.getForm().findField('name').getValue();
		var formNumber = form1.getForm().findField('number').getValue();
		var formDefault = form1.getForm().findField('rb').getValue();
		form1.load({
			url: '/pdms/pm/project-temp!getProjectTemplateAdd.action',
			params: {
				templateName: formName,
				templateNumber: formNumber,
				templateDefault: formDefault
			}
		});
    },

    //取消
    cancel: function(btn) {
    	btn.up('projectTemplateAdd').destroy();
    }

});