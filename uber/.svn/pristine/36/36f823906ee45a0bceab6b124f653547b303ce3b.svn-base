/**
 * 项目管理-车型项目
 */
Ext.define('ExtApp.view.projectManagement.vehicle.VehicleBase',{
	extend:'Ext.tab.Panel',
	xtype:'vehicleBase',
	
	layout: 'fit',

	initComponent: function() {
		var me = this;
		
		if (me.record.get('planStatus') == 'ACTIVE') {
			this.items = [{
				xtype: 'vehicleDashboard',
				programId: me.programId,
				programVehicleId: me.programVehicleId,
				projectPrivilege: me.projectPrivilege
			},{
				title:'基本信息',
				xtype: 'deployPlan',
				programId: me.programId,
				programVehicleId: me.programVehicleId,
				projectPrivilege: me.projectPrivilege
			},{
				xtype: 'projectDBStatusReport',
				programId: me.programId,
	        	programVehicleId: me.programVehicleId,
	        	projectPrivilege: me.projectPrivilege
			}];
		} else {
			this.items = [{
				title:'基本信息',
				xtype: 'deployPlan',
				programId: me.programId,
				programVehicleId: me.programVehicleId,
				projectPrivilege: me.projectPrivilege
			},{
				xtype: 'vehicleDashboard',
				programId: me.programId,
				programVehicleId: me.programVehicleId,
				projectPrivilege: me.projectPrivilege
			},{
				xtype: 'projectDBStatusReport',
				programId: me.programId,
	        	programVehicleId: me.programVehicleId,
	        	projectPrivilege: me.projectPrivilege
			}];
		}
		this.callParent();
	}
});