.timelinepanel {

  .function.sch-event .task {
    height: 25px !important;
    top: 30px !important;
    overflow: visible;
    background: lightblue;
  }

  .function.sch-event-milestone .task {
    top: 0px !important;
    background: transparent;
  }

  .function .task-milestone {

    @extend .milestone-wrap;

    position: absolute;
    top:  -30px;

    .task-milestone-inner {
      @extend .milestone-triangle;
    }
  }

  .task-milestone-body {
    color: black;
  }

}