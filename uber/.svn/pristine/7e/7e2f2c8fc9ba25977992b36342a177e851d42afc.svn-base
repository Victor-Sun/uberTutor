/**
 * 部门空间-文档管理-新建文件夹
 */
Ext.define('ExtApp.view.deptworkspace.document.CreateDeptFolder',{
	extend:'Ext.window.Window',
	xtype:'createDeptFolder',

	controller:'createDeptFolder',

	modal: true,
	closable: false,
	
	layout: 'fit',
	
	width: 300,
	height: 150,
	
	title: '新建文件夹',
	
	initComponent: function() {
		var me = this;
		this.items = [{
			xtype: 'panel',
			margin: 10,
			layout:{
				type:'vbox',
				align:'stretch'
			},
			items: [{
				xtype: 'textfield',
				itemId: 'folderName',
				fieldLabel: '文件夹名称'
			}]
		}];
		this.tools = [{
			text:'确定',
			xtype: 'button',
			iconCls: 'x-fa fa-circle-o',
			handler: 'confirm'
		},{
			text:'关闭',
			xtype: 'button',
			iconCls: 'x-fa fa-close',
			handler: 'cancel'
		}];
		this.callParent(arguments);
	}
});