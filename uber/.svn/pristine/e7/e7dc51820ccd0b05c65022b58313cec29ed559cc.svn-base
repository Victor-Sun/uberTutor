    position : static !important;
}

/* Make sure row element doesn't make dependency line unreachable
 https://www.assembla.com/spaces/bryntum/tickets/2391
/* See tests 201_create.t.js and 052_dependency_events.t.js */
.sch-ganttview .x-grid-item-focused .x-grid-cell-inner {
