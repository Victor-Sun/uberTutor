					flex: 1,
					dataIndex:'taskName'
				},{
					text:'交付物编号',
					flex: 1,
					dataIndex:'code'
				},{
					text:'交付物名称',
					flex: 1,
					dataIndex:'name'
				},{
					text:'检查项目',
					flex: 1,
					dataIndex:'checkitemName'
				},{
					text:'单项通过要求及验收办法',
					flex: 1,
					dataIndex:'checkRequirement'
				},{
					text:'负责主体',
					flex: 1,
					dataIndex:'obsName'
				},{
					text:'是否关键交付物',
					flex: 1,
					dataIndex:'isKey'
				}],
