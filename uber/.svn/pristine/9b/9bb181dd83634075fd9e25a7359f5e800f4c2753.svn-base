/**
 * 项目管理-专项-试验计划
 */
Ext.define('ExtApp.view.projectManagement.extplan.ProjectExtensionPlan01',{
	extend:'Ext.tab.Panel',
	xtype:'projectExtensionPlan01',
	
	scrollable:'y',
	
	initComponent: function() {
		// 专项基本信息
		var planInfo = Ext.create(
				'ExtApp.view.projectManagement.extplan.ProjectExtensionPlanInfo',
				{extProjectId: this.nodeId});
		planInfo.load({
			url: '/pdms/pm/project-extension-plan!getExtensionPlanInfo.action',
			params: {extProjectId: this.nodeId}
		});
		
		// 专项列表信息
		var list = Ext.create(						
				'ExtApp.view.projectManagement.extplan.ProjectExtensionPlan01List',
				{extProjectId: this.nodeId});

		this.items = [planInfo, list];
		this.callParent(arguments);
	}	    	
});