/**
 * 首页-个人设置-常用项目Controller
 */
Ext.define('ExtApp.view.dashboard.DashCommonProjectController', {
	extend: 'ExtApp.view.common.BaseViewController',
    alias: 'controller.dashCommonProject',
    
    //双击列表
    onCelldblclick: function(grid , td , cellIndex , record , tr , rowIndex , e , eOpts) {
    	var rec = grid.getStore().getAt(rowIndex);
    	var win = Ext.create({
    		xtype: 'projectWindow',
    		programId: rec.get('id'),
			programCode: rec.get('code'),
			programName: rec.get('programName')
    	});
    	win.show();
    }
});
