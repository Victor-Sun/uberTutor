/**
 * 项目组织查询
 */
Ext.define('ExtApp.view.common.obs.PmObs',{
	extend:'Ext.window.Window',
	xtype:'pmObs',
	
	modal: true,
	closable: false,
	
	controller:'pmObs',
	
	width: 300,
	height: 500,
	
	title: '项目组织查询',
	
	initComponent: function() {
		var useStore = Ext.create('ExtApp.store.common.PmObsStore');
		useStore.getProxy().extraParams = {
			programId: this.programId, vehicleId: this.vehicleId, rootObsId: this.rootObsId};
		useStore.load();
	
		this.items = [{
			xtype:'panel',
			layout: 'fit',
			height: 400,
			scrollable:'y',
			margin:10,
			items:[{
				xtype:'treepanel',
				rootVisible: false,
                store:useStore
			}]
		}];
		this.tools = [{
			text:'确定',
			xtype: 'button',
			iconCls: 'x-fa fa-circle-o',
			handler: 'confirm'
		},{
			text:'关闭',
			xtype: 'button',
			iconCls: 'x-fa fa-close',
			handler: 'cancel'
		}];
		this.callParent(arguments);
	}
});