/**
 * 项目组织查询
 */
Ext.define('ExtApp.view.common.obs.PmObsController', {
    extend: 'Ext.app.ViewController',
    alias: 'controller.pmObs',
    
	// 取消
	cancel: function(btn) {
		this.view.close();
	},
	
    // 选择用户确定
    confirm: function(btn) {
    	var tree = btn.up('window').down('treepanel');
    	var model = tree.getSelection();
    	if (model.length > 0) {
    		this.view.parentCaller.confirmObs(model[0]);
    	}
    	this.cancel();
    }

});