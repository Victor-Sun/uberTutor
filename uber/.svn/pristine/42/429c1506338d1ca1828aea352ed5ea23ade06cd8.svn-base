/**
 * 首页-个人设置View
 */
Ext.define('ExtApp.view.toolbarsettings.PersonalSettings',{
	extend:'Ext.window.Window',
	xtype:'personalSettings',

	viewModel: 'personalSettings',
	controller: 'personalSettings',

	modal: true,
	width:800,
	height:500,
	layout: 'fit',

	initComponent: function() {

		this.items = [{
			xtype:'tabpanel',
			items:[{
				title: '常用项目',
				xtype:'programUsual'
			},{
				title: '密码设定',
				xtype:'programChangePws'
			},{
				title: '工作组',
				xtype: 'myWorkGroup'
			}]
		}];
		this.callParent(arguments);
	}
});