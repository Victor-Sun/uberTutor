/**
 * 项目管理-基本信息-项目备案记录
 */
Ext.define('ExtApp.view.projectmanagement.base.ProjectBaseInfo03',{
	extend:'Ext.panel.Panel',
	xtype:'projectBaseInfo03',

	margin:20,

	initComponent: function() {

	    this.items = [{
	    	xtype:'fieldset',
	    	title: '<B>项目备案记录</B>',
	    	collapsible:true,
	    	items:[{
	    		xtype: 'panel',
				items: [{
					xtype: 'fileAttachment',
					programId: this.programId,
		    		sourceType: 'SOURCE_TYPE_PM_RECORD',
		    		sourceId: this.programId,
		    		editable: this.editPrivilege
				}]
	    	}]
	    }];
		this.callParent(arguments);
	}
});