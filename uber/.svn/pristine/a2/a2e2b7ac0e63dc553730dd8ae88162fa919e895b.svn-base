Ext.define('ExtApp.view.projectmanagement.org.ProjectOrganization03',{
	extend:'Ext.panel.Panel',
	xtype:'projectOrganization03',
	
	controller:'projectOrganizationTab',
	
	layout: 'fit',
	
	initComponent: function() {
		// 二级组织架构编辑权限
		this.editPrivilege = ExtApp.util.Util.getPrivilege(
				'P0605', this.projectPrivilege, this.programVehicleId);
		// 查看基线页面不能编辑
		if (this.baselineId && this.baselineId != null) {
			this.editPrivilege = false;
		}
		var useStore = Ext.create(
			'ExtApp.store.projectmanagement.ProjectOrganization03Store');
		useStore.getProxy().extraParams = {programId: this.programId,
			programVehicleId: this.programVehicleId,
			baselineId: this.baselineId};
		useStore.load();
	
		this.items = [{
			xtype:'panel',
			margin:10,
			layout:{
				type:'hbox',
				align:'stretch'
			},
			items:[{
				xtype:'panel',
				layout: 'fit',
				flex:1,
				margin:5,
				cls:'shadow',
				items:[{
					xtype:'treepanel',
					scrollable:'y',
					rootVisible: false,
                    store:useStore,
					listeners:{
						selectionchange: 'onSelectionchange',
						itemcontextmenu: 'showMenuLevelTwo'
					}
				}]
			},{
				xtype:'panel',
				margin:5,
				cls:'shadow',
				flex:3,
				reference: 'mainCardPanel',
				layout: {
					type: 'card'
				}
			}]
		}];
//		this.tbar = [{
