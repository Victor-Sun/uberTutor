/**
 * 质量问题管理-问题提交-描述附件Controller
 */
Ext.define('ExtApp.view.qualityissuemanagement.qesubmission.DesAttachmentController', {
    extend: 'Ext.app.ViewController',
    alias: 'controller.desAttachment',

	// 点击【添加附件】
	addFile: function() {
    	var me = this;
    	this.dialog = me.view.add({
			title:"上传",
        	xtype: 'desAttachmentWindow'
    	});
    	this.dialog.show();
    },
	// 点击【删除】
    fileDelete: function(btn) {
		btn.ownerCt.destroy();
	}
});